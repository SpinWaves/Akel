// ================ FOR GCC COMPILATION ================ //

/* Used to get informations from the CPU */

"cpuid" 
: "=a" (regs[0]), 
  "=b" (regs[1]), 
  "=c" (regs[2]), 
  "=d" (regs[3])
             
: "a" (funcId), 
  "c" (subFuncId)
